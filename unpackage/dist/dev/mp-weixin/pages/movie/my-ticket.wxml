<view><view class="tick-tab-list"><block wx:for="{{tabList}}" wx:for-item="value" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getTab',[index]]]]]}}" class="{{['tick-tab-item',(index==tabIndex)?'tab-sel':'']}}" bindtap="__e">{{''+value.text+''}}<block wx:if="{{index==tabIndex}}"><view class="bottoms"></view></block></view></block></view><block wx:for="{{dataList}}" wx:for-item="value" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['getDetails',['$0'],[[['dataList','',index]]]]]]]}}" class="tick-lists" bindtap="__e"><view class="tick-list-top"><view class="tock-shop-name">{{value.movie_info.cinemaName}}</view><block wx:if="{{value.status==0}}"><view class="tock-list-status">待支付</view></block><block wx:if="{{value.status==1}}"><view class="tock-list-status">待出票</view></block><block wx:if="{{value.status==2}}"><view class="tock-list-status">已出票</view></block><block wx:if="{{value.status==3}}"><view class="tock-list-status">出票失败</view></block><block wx:if="{{value.status==4}}"><view class="tock-list-status">已完成</view></block><block wx:if="{{value.status==5}}"><view class="tock-list-status">售后</view></block></view><view class="tick-list-info"><view class="film-cover"><image src="{{value.movie_info.posterUrl}}"></image></view><view class="film-list-ret"><view class="film-name">{{value.movie_info.movie_name}}</view><view class="film-time">{{value.showInfor.length+"张 I "+value.movie_info.time}}</view></view></view></view></block></view>